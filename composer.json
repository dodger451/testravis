{
    "name": "dodger451/testravis",
    "description": "Test running Travis",
    "keywords": ["travis", "template"],
    "homepage": "http://david.latotzky.de",
    "version": "1.0",
    "authors": [
        {
            "name": "David Latotzky",
            "homepage": "http://david.latotzky.de/dev"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpmd/phpmd" : "@stable",
        "satooshi/php-coveralls": "dev-master",
        "phpunit/phpunit": "~4.5"
    },
    "recommend": {
    },
    "autoload": {
        "psr-4": {
            "Latotzky\\Testravis\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Latotzky\\Testravis\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
